<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.StackPane?>

<StackPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="userWindow.UserWindowController">
   <children>
      <TableView fx:id="userTableView" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="columnId" prefWidth="75.0" text="id" />
          <TableColumn fx:id="columnUsername" prefWidth="75.0" text="username" />
            <TableColumn fx:id="columnSex" prefWidth="75.0" text="sex" />
            <TableColumn fx:id="columnCar" prefWidth="75.0" text="car model" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</StackPane>
